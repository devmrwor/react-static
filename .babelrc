{
  "presets": [
    [
      "latest",
      {
        "loose": true,
        "modules": false
      }
    ],
    "stage-0",
    "stage-3",
    "react"
  ],
  "ignore": "app.static.js",
  "plugins": [
    "react-hot-loader/babel",
    "transform-class-properties",
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ]
  ]
}
