{
  "name": "react-static-plugin-mdx",
  "version": "0.1.0",
  "description": "A plugin for React Static to transform MDX files (Markdown + React components).",
  "main": "node.api.js",
  "repository": "https://github.com/nozzle/react-static.git",
  "author": "Matteo Frana <f2@f2.net>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "babel src --out-dir .",
    "watch": "babel src --out-dir . -w",
    "test": "yarn format",
    "format": "prettier 'src/**/*.js' --write",
    "preversion": "yarn build"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.1.5"
  },
  "dependencies": {
    "@mdx-js/loader": "^0.16.6"
  }
}
