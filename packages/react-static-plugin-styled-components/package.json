{
  "name": "react-static-plugin-styled-components",
  "version": "6.1.0",
  "description": "A React-Static plugin that adds CSS-in-JS/SSR support for styled-components",
  "main": "node.api.js",
  "repository": "https://github.com/nozzle/react-static-plugin-styled-components.git",
  "author": "Tanner Linsley <tannerlinsley@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "babel src --out-dir .",
    "watch": "babel src --out-dir . -w",
    "test": "yarn format",
    "format": "prettier 'src/**/*.js' --write",
    "preversion": "yarn build"
  },
  "peerDependencies": {
    "styled-components": "^4.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0"
  },
  "gitHead": "ca9f2162257fe67a8b4ffcbb1350695345940da4"
}
