{
  "name": "react-static-plugin-jss",
  "version": "6.1.0",
  "description": "A React-Static plugin that adds CSS-in-JS/SSR support for jss",
  "main": "node.api.js",
  "repository": "https://github.com/nozzle/react-static.git",
  "author": "Tanner Linsley <tannerlinsley@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "babel src --out-dir .",
    "watch": "babel src --out-dir . -w",
    "test": "yarn format",
    "format": "prettier 'src/**/*.js' --write",
    "preversion": "yarn build"
  },
  "peerDependencies": {
    "react-jss": "^8.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0"
  }
}
