{
  "name": "react-static-plugin-sitemap",
  "version": "7.6.1",
  "description": "A React-Static plugin for exporting sitemap information",
  "main": "dist/node.api.js",
  "repository": "https://github.com/react-static/react-static.git",
  "author": "Tanner Linsley <tannerlinsley@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "babel src --out-dir ./dist --ignore src/**/*.spec.js,src/**/*.test.js",
    "watch": "yarn build --watch",
    "start": "yarn watch",
    "test": "yarn format && yarn unitTest",
    "unitTest": "jest",
    "format": "prettier src/**/*.js --write",
    "preversion": "yarn build"
  },
  "peerDependencies": {
    "react-static": "^7.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "react-static": "^7.6.1"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "fs-extra": "^8.1.0"
  },
  "gitHead": "875004e6e75d36043382b8ec8bba27a766c1a83a"
}
