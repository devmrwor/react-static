{
  "name": "react-static-plugin-sitemap",
  "version": "7.0.10",
  "description": "A React-Static plugin for exporting sitemap information",
  "main": "dist/node.api.js",
  "repository": "https://github.com/nozzle/react-static.git",
  "author": "Tanner Linsley <tannerlinsley@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "babel src --out-dir ./dist --ignore src/**/*.spec.js,src/**/*.test.js",
    "watch": "yarn build --watch",
    "start": "yarn watch",
    "test": "yarn format && yarn unitTest",
    "unitTest": "jest",
    "format": "prettier src/**/*.js --write",
    "preversion": "yarn build"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.2"
  },
  "gitHead": "ca9f2162257fe67a8b4ffcbb1350695345940da4"
}
